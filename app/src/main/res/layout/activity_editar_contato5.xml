<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MinhaAgendaV5.EditarContato5Activity">

    <EditText
        android:id="@+id/agenda5_txtNome"
        android:layout_width="347dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="104dp"
        android:ems="10"
        android:hint="Nome:"
        android:inputType="textPersonName"
        android:nextFocusDown="@id/agenda5_txtTelefone"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.75"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/agenda5_txtSobreNome"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:nextFocusDown="@id/agenda5_txtNome"
        android:text="SobreNome:"
        app:layout_constraintEnd_toEndOf="@+id/agenda5_txtNome"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/agenda5_txtNome"
        app:layout_constraintTop_toBottomOf="@+id/agenda5_txtNome" />

    <EditText
        android:id="@+id/agenda5_txtTelefone"
        android:layout_width="347dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:ems="10"
        android:hint="Telefone:"

        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="@+id/agenda5_txtSobreNome"
        app:layout_constraintStart_toStartOf="@+id/agenda5_txtSobreNome"
        app:layout_constraintTop_toBottomOf="@+id/agenda5_txtSobreNome" />

    <EditText
        android:id="@+id/agenda5_txtemail"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:ems="10"
        android:inputType="textEmailAddress"
        android:text="E-mail"
        app:layout_constraintEnd_toEndOf="@+id/agenda5_txtSobreNome"
        app:layout_constraintStart_toStartOf="@+id/agenda5_txtSobreNome"
        app:layout_constraintTop_toBottomOf="@+id/agenda5_txtTelefone" />

    <EditText
        android:id="@+id/agenda5_txtEndereço"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:nextFocusDown="@id/agenda5_txtNome"
        android:text="Endereço:"
        app:layout_constraintStart_toStartOf="@+id/agenda5_txtemail"
        app:layout_constraintTop_toBottomOf="@+id/agenda5_txtemail" />


    <Button
        android:id="@+id/agenda5_btSalvar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:backgroundTint="#058F06"
        android:text="Salvar"
        app:layout_constraintEnd_toEndOf="@+id/agenda5_txtemail"
        app:layout_constraintStart_toStartOf="@+id/agenda5_txtemail"
        app:layout_constraintTop_toBottomOf="@+id/agenda5_txtEndereço" />

    <TextView
        android:id="@+id/txt5Nome"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nome:"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/agenda5_txtNome"
        app:layout_constraintStart_toStartOf="@+id/agenda5_txtNome" />

    <TextView
        android:id="@+id/txt5SobreNome"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="SobreNome"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/agenda5_txtSobreNome"
        app:layout_constraintStart_toStartOf="@+id/agenda5_txtSobreNome" />

    <TextView
        android:id="@+id/txt5telefone"
        android:layout_width="75dp"
        android:layout_height="0dp"
        android:text="Telefone"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/agenda5_txtTelefone"
        app:layout_constraintStart_toStartOf="@+id/agenda5_txtTelefone" />

    <TextView
        android:id="@+id/txt5Email"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="E-mail"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/agenda5_txtemail"
        app:layout_constraintStart_toStartOf="@+id/agenda5_txtemail" />

    <TextView
        android:id="@+id/txt5Endereço"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Endereço"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/agenda5_txtEndereço"
        app:layout_constraintStart_toStartOf="@+id/agenda5_txtEndereço" />
</androidx.constraintlayout.widget.ConstraintLayout>